{
  "name": "umi",
  "scope": "umijs",
  "version": 2,
  "builds": [
    {
      "src": "./website/package.json",
      "use": "@now/static-build",
      "config": {
        "includeFiles": "docs/**",
        "distDir": "./dist"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "website/$1" }],
  "github": {
    "enabled": false
  },
  "alias": ["v2.umijs.org"]
}
